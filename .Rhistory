## Function cacheSovle is to return the inversed matrix
cacheSolve <- function(x, ...) {
## Return a matrix that is the inverse of 'x'.
m<-x$getInverse()
if(!is.null(m))
{
message("getting cached data")
return(m)
}
data<-x$get()
m<-solve(data,...)
x$setInverse(m)
m
}
